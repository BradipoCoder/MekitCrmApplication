{
    "name": "mekit/crm-application",
    "description": "Mekit CRM Application",
    "homepage": "https://github.com/adamjakab/MekitCrmApplication.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Mekit",
            "homepage": "http://www.mekit.it"
        }
