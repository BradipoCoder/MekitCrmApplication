{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set gridName = 'tasks-grid' %}
{% set pageTitle = 'mekit.task.entity_plural_label'|trans %}

{% block navButtons %}
	{% if resource_granted('mekit_task_create') %}
		<div class="btn-group">
			{{ UI.addButton({
			'path' : path('mekit_task_create'),
			'entity_label': 'mekit.task.entity_label'|trans
			}) }}
		</div>
	{% endif %}
{% endblock %}