{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set gridName = 'contacts-grid' %}
{% set pageTitle = 'mekit.contact.entity_plural_label'|trans %}


{% block navButtons %}
	{% if resource_granted('mekit_contact_create') %}
		<div class="btn-group">
			{{ UI.addButton({
			'path' : path('mekit_contact_create'),
			'entity_label': 'mekit.contact.entity_label'|trans
			}) }}
		</div>
	{% endif %}
{% endblock %}
