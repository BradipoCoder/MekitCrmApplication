{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set gridName = 'accounts-grid' %}
{% set pageTitle = 'mekit.account.entity_plural_label'|trans %}


{% block navButtons %}
	{% if resource_granted('mekit_account_account_create') %}
		<div class="btn-group">
			{{ UI.addButton({
			'path' : path('mekit_account_create'),
			'entity_label': 'mekit.account.entity_label'|trans
			}) }}
		</div>
	{% endif %}
{% endblock %}
